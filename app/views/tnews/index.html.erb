<h1>Listing tnews</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tnews.each do |tnews| %>
  <tr>
    <td><%= tnews.title %></td>
    <td><%= tnews.content %></td>
    <td><%= link_to 'Show', tnews %></td>
    <td><%= link_to 'Edit', edit_tnews_path(tnews) %></td>
    <td><%= link_to 'Destroy', tnews, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tnews', new_tnews_path %>
